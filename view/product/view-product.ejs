<%-include('../Includes/header.ejs')%>
</head>
<body>
<header><%-include('../Includes/navbar.ejs')%></header>
<main>
  <section class="py-5">
    <div class="container">
      <table border="2">
        <tr>
          <th>Product Name</th>
          <th>Product Price</th>
          <th>Category Name</th>
        </tr>
        <%allProduct.map(data => {%>
        <tr>
          <td><%= data.productName%></td>
          <td><%= data.productPrice%></td>
          <td><%= data.category.category%></td>
        </tr>
        <%})%>
      </table>
    </div>
  </section>
</main>
<%-include('../Includes/footer.ejs')%>
